# Docker Compose Environment Variables
# Copy this file to .env and configure your API keys

# Required for OCR processing
MISTRAL_API_KEY=your_mistral_api_key_here

# Required for AI extraction (choose one)
OPENAI_API_KEY=your_openai_api_key_here
# OR
LANGEXTRACT_API_KEY=your_openai_api_key_here

# Optional configuration
DEBUG=false

# Docker Compose profiles (uncomment to enable)
# COMPOSE_PROFILES=ollama        # Enable Ollama local LLM
# COMPOSE_PROFILES=cache         # Enable Redis caching
# COMPOSE_PROFILES=ollama,cache  # Enable both
